{
  "name": "Aquarius",
  "version": "1.0",
  "manifest_version": 3,
  "description": "users can ask and get answers to any question. Supports multiple languages and topics.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "sidepanel/index.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
